  "version": "0.2.0",
  "configurations": [
    {
      "name": "run blockrewardtest",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/truffle/build/cli.bundled.js",
      "args": ["test"],
      "cwd": "${workspaceRoot}",
      "outFiles": [
          "${workspaceRoot}/test/BlockRewardHbbft.js",
          ],
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/lib/**/*.js",
        "<node_internals>/**/*.js"
      ],
      "env": {
        "CONTRACTS_NO_UPGRADE_PROXY": "true"
      }
    },
    {
      "name": "run stakingTest",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/truffle/build/cli.bundled.js",
      "args": ["test"],
      "cwd": "${workspaceRoot}",
      "outFiles": [
          "${workspaceRoot}/test/StakingHbbft.js",
          ],
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/lib/**/*.js",
        "<node_internals>/**/*.js"
      ]
    },
    {
      "name": "run testDev",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/truffle/build/cli.bundled.js",
      "args": ["testDev"],
      "cwd": "${workspaceRoot}",
      "outFiles": [],
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/lib/**/*.js",
        "<node_internals>/**/*.js"
      ]
    },
    {
      "name": "makeSpec",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "make_spec_test"]
    }
    {
      "name": "npm run testDev",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "outFiles": [],
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "testDev"],
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/lib/**/*.js",
        "<node_internals>/**/*.js"
      ]
    },
    
  ]
